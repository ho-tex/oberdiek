\href{gotoe:%
  dest={page.1},parent%
}{Go to first page of main document}\\
\href{gotoe:%
  dest={page.2},parent%
}{Go to second page of main document}
\newpage
\section{This is the second page of the child document.}
\href{gotoe:%
  dest={page.1},parent%
}{Go to first page of main document}\\
\href{gotoe:%
  dest={page.2},parent%
}{Go to second page of main document}

\hypertarget{foobar}{}
Anker foobar is here.
\end{document}
\end{filecontents}
